div(class='cover-container contentWrapper core-value')
    div(class='container')
        div(class='row')
          div(class='col')
            h1= langText.coreValue.title
            hr(class='yellow')
            div(class='row justify-content-around')
              each item in langText.coreValue.items
                div(class='col-12 text-center')
                  img(src!='../images/' + item.icon)
                  h2= item.caption
                  div(class='text-left itemContent')= item.description